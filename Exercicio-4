programa
{
	
	funcao inicio()
	{
		inteiro valordahora, horatrabalhada, salario
		

		escreva("Digite valor da hora: ")
		leia(valordahora)
		escreva("Digite a quantidade de horas trabalhadas: ")
		leia(horatrabalhada)

		salario = valordahora * horatrabalhada

	     escreva("O seu saláro é: ", salario)

		
	}
}
